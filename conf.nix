{
  # Directory where posts are located
  postsDir = ./posts;

  # Directory where drafts are located
  draftsDir = ./drafts;

  # Directory where templates are located
  templatesDir = ./templates;

  # Directory where static files are located
  staticDir = ./static;

  # Site id, used in the caching scheme. [a-z0-9\-_]+
  # Default value can be used
  siteId = "styx";

  # URL of the site, must be set to the url of the domain the site will be deployed
  siteUrl = "https://styx-static.github.io/styx-site";

  # Site title, used in the site header and the atom feed
  siteTitle = "Styx";

}
